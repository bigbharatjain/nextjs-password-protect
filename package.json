{
  "name": "nextjs-password-protect",
  "version": "1.0.1",
  "description": "A reusable password protection module for Next.js applications with secure token-based authentication",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api-route-template": {
      "import": "./dist/api-route-template.js",
      "require": "./dist/api-route-template.js",
      "types": "./dist/api-route-template.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "api-route-template.ts",
    "index.ts",
    "context.tsx",
    "PasswordProtect.tsx",
    "PasswordProtectWrapper.tsx",
    "types.ts",
    "tsconfig.build.json",
    "README.md",
    "SETUP.md",
    "EXAMPLE.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo 'No tests specified'"
  },
  "keywords": [
    "password",
    "protection",
    "authentication",
    "nextjs",
    "next.js",
    "react",
    "security",
    "token",
    "auth",
    "password-protect",
    "password-protection"
  ],
  "author": "bigbharatjain",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bigbharatjain/nextjs-password-protect.git"
  },
  "bugs": {
    "url": "https://github.com/bigbharatjain/nextjs-password-protect.git/issues"
  },
  "homepage": "https://github.com/bigbharatjain/nextjs-password-protect.git",
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  }
}
