{
  "name": "nextjs-password-protect",
  "version": "1.0.0",
  "description": "A reusable password protection module for Next.js applications with secure token-based authentication",
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./api-route-template": "./api-route-template.ts"
  },
  "files": [
    "*.ts",
    "*.tsx",
    "README.md",
    "SETUP.md",
    "EXAMPLE.md"
  ],
  "scripts": {
